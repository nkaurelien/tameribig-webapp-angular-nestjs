{
  "name": "tameribig-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rimraf dist && cross-env NODE_OPTIONS=--max_old_space_size=2460 tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "starter": "ts-node -r tsconfig-paths/register src/main.ts",
    "serve": "nest build --watch --webpack --webpackPath webpack-hmr.config.js",
    "serve:hmr": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "starter:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "starter:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "npm run build",
    "starter:prod": "node -v && node --optimize_for_size --max_old_space_size=2460 dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "npm run prestart:prod"
  },
  "dependencies": {
    "@bugsnag/js": "^7.1.0",
    "@bugsnag/plugin-express": "^7.1.0",
    "@google-cloud/storage": "^3.5.0",
