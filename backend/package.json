  "_moduleAliases": {
    "@root": ".",
    "@api": "dist/@api",
    "@core": "dist/@core",
    "@documentation": "dist/@documentation",
    "@schemas": "dist/@schemas",
    "@databases": "dist/databases",
    "@environments": "dist/environments"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
