    "@nestjs/swagger": "4.7.13",
    "@nkaurelien/nest-bugsnag": "^6.1.0",
    "archiver": "^3.1.1",
    "aws-sdk": "^2.681.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "cloudinary": "^1.21.0",
    "cookie-parser": "latest",
    "csurf": "^1.11.0",
    "date-fns": "^2.14.0",
    "debug": "^4.1.1",
